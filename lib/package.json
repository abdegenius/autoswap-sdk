{
  "name": "autoswap-sdk",
  "version": "1.0.6",
  "description": "TypeScript SDK for interacting with AutoSwappr function",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "starknet",
    "cairo",
    "defi",
    "swap",
    "ekubo",
    "autoswappr"
  ],
  "author": "Ejembi Emmanuel",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "starknet": "^5.19.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BlockheaderWeb3-Community/autoswap-sdk.git"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
